{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:01:00",
      "visibilityTimeout": "00:00:15",
      "batchSize": 8,
      "maxDequeueCount": 4,
      "newBatchThreshold": 2,
      "messageEncoding": "base64"
    }
  }
}